#!/usr/bin/env php
<?php

define('ROOT_DIR', dirname(__DIR__));
// Load composer
require_once(ROOT_DIR. '/vendor/autoload.php');


use Symfony\Component\Console\Application;

$app = new Application('SugarCRM command line', 'v0.1.0');
$app->add(new \SugarCli\Install\CheckCommand);
$app->add(new \SugarCli\Install\RunCommand);
$app->add(new \SugarCli\Install\GetConfigCommand);

$app->run();


