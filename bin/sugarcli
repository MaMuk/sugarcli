#!/usr/bin/env php
<?php

// Load composer
require_once(dirname(__DIR__) . '/vendor/autoload.php');


use Symfony\Component\Console\Application;

$app = new Application('SugarCRM command line', '@sugarcli_version@');
$app->add(new \SugarCli\Install\CheckCommand);
$app->add(new \SugarCli\Install\RunCommand);
$app->add(new \SugarCli\Install\GetConfigCommand);

$app->add(new \SugarCli\Clean\LangFilesCommand);

$app->run();


